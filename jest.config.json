{
    "setupFiles": ["<rootDir>/src/setupTests.js"],
    "testRegex": "/*.test.js$",
    "collectCoverage": true,
    "coverageReporters": ["lcov"],
    "coverageDirectory": "test-coverage",
    "coverageThreshold": {
     "global": {
     "branches": 0,
     "functions": 0,
     "lines": 0,
     "statements": 0
     }
    },
    "globals": {
        "TZ": "utc"
    },
    "moduleDirectories": ["node_modules", "src"]
   }